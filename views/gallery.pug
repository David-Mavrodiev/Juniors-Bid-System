extends index

block content
  .container
    .row
     .col-lg-12
       h1.page-header Auctions Gallery
     each auction in result.auctions  
       .col-lg-3.col-md-4.col-xs-6.thumb
         a.thumbnail(href='/auctions/' + auction.id)
          img.img-responsive(src="/static/auctionimages/" + auction.id + ".jpg", alt='', style="width: 400px; height: 200px;")